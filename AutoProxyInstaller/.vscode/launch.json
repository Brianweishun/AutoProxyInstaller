
{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Node App",
      "program": "${workspaceFolder}/server.js",
      "skipFiles": ["<node_internals>${/**"],
      "console": "integratedTerminal"
    },
	{
      "type": "chrome",
      "request": "launch",
      "name": "Debug Browser",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
